		{
			"name": "PUT Policy v1.0.0",
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "package httpapi.authz\n\n# bob is alice's manager, and betty is charlie's.\nsubordinates = {\"alice\": [], \"charlie\": [], \"bob\": [\"alice\"], \"betty\": [\"charlie\"]}\n\n# HTTP API request\nimport input\n\ndefault allow = false\n\n# Allow users to get their own salaries.\nallow {\n  input.method == \"GET\"\n  input.path = [\"finance\", \"salary\", username]\n  input.user == username\n}\n\n# Allow managers to get their subordinates' salaries.\nallow {\n  input.method == \"GET\"\n  input.path = [\"finance\", \"salary\", username]\n  subordinates[input.user][_] == username\n}"
				},
				"url": {
					"raw": "http://localhost:8181/v1/policies/example",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8181",
					"path": [
						"v1",
						"policies",
						"example"
					]
				}
			},
			"response": []
		},
